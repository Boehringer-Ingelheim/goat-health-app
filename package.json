{
  "name": "goat-health",
  "version": "1.0.0",
  "private": true,
  "description": "Making More Health - Goat Health",
  "author": "Making More Health | Simon Golms",
  "homepage": "https://www.makingmorehealth.org/",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "generate:images": "node scripts/images/squoosh.js",
    "generate:release": "semantic-release",
    "generate:screenshots": "",
    "generate:resources": "node scripts/resources.js",
    "generate:resources:assets": "npm run generate:resources:assets:android && npm run generate:resources:assets:ios && npm run generate:resources:assets:pwa",
    "generate:resources:assets:android": "node scripts/resources/assets-android.js",
    "generate:resources:assets:ios": "node scripts/resources/assets-ios.js",
    "generate:resources:assets:pwa": "node scripts/resources/assets-pwa.js",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:ionic:browser": "ionic build --prod --engine=browser",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "res": "node scripts/res.js",
    "repair": "npx rimraf build node_modules .git/hooks package-lock.json && npm install",
    "stylelint-check": "stylelint-config-prettier-check"
  },
  "dependencies": {
    "@capacitor/android": "^2.4.5",
    "@capacitor/core": "^2.4.5",
    "@capacitor/ios": "^2.4.5",
    "@ionic/react": "^5.5.2",
    "@ionic/react-router": "^5.5.2",
    "@ionic/storage": "^2.3.1",
    "@reduxjs/toolkit": "^1.5.0",
    "@testing-library/jest-dom": "5.11.8",
    "@testing-library/react": "^11.2.3",
    "@testing-library/user-event": "^12.6.0",
    "connected-react-router": "6.8.0",
    "i18next": "19.8.4",
    "i18next-browser-languagedetector": "6.0.1",
    "ionicons": "5.3.0",
    "lunr": "^2.3.9",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-i18next": "11.8.5",
    "react-redux": "7.2.2",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.1",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@capacitor/cli": "^2.4.5",
    "@ionic/cli": "^6.12.3",
    "@semantic-release/git": "^9.0.0",
    "@squoosh/cli": "^0.6.1",
    "@types/jest": "^26.0.20",
    "@types/lunr": "^2.3.3",
    "@types/node": "^14.14.20",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.15",
    "@types/react-router": "^5.1.10",
    "@types/react-router-dom": "5.1.7",
    "chalk": "^4.1.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.3.7",
    "lint-staged": "^10.5.3",
    "mobicon": "3.0.0",
    "mobisplash": "0.2.2",
    "ora": "^5.2.0",
    "playwright": "1.7.1",
    "prettier": "^2.2.1",
    "semantic-release": "^17.3.3",
    "stylelint": "^13.8.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-prettier": "^1.1.2"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
